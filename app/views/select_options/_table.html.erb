  <table class="table table-striped table-bordered table-responsive mt-2">
    <thead>
      <tr>
        <th scope="col"><%= SelectOption.model_name.human(count: 2) %></th>
        <th scope="col"><%= t("actions") %></th>
      </tr>
    </thead>
    <tbody>
      <% category.select_options.each do |select_option|  %>
        <tr>
          <td>
            <%= select_option.name %>
          </td>
          <td>
            <%= link_to edit_select_option_path(select_option), class: "action_button", data: { cy: "edit_select_option" } do %>
            <i class="bi bi-pencil-square"></i> <%= t("edit") %>
          <% end %>
            <br>
            <%= link_to select_option, method: :delete, data: { confirm: 'Are you sure?', cy: "delete_select_option" }, class: "action_delete_button" do %>
              <i class="bi bi-x-square"></i> <%= t("delete") %>
            <% end %>
          </td>
        </tr>
      <% end %>
      <tr>
        <td colspan="2">
          <strong>
            <%= SelectOption.human_attribute_name("enter", select_option: SelectOption.model_name.human) %>
          </strong>
          <%= render 'select_options/form', select_option: SelectOption.new, category: category %>
        <td>
      </tr>
    </tbody>
  </table>

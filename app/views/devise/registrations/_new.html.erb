<%= simple_form_for resource, as: resource_name, url: registration_path(resource_name), html: { data: { cy: "form_for_registration" }} do |f| %>
<h1><%= t("signup") %></h1>
<p>
  <%= f.input :email%>
  <br>
  <%= f.input :password %>
  <br />
  <%= f.input :password_confirmation %>
</p>
  <%= f.submit "Anmelden", class: "primary-button" %>
<% end %>
